{
  "name": "rabbitcms/settings",
  "description": "Settings module for RabbitCMS",
  "require": {
    "rabbitcms/carrot": "^0.3.0"
  },
  "type": "october-module",
  "authors": [
    {
      "name": "Kulyk Dmytro",
      "email": "lnkvisitor.ts@gmail.com"
    },
    {
      "name": "Dimon Zhurakovskiy",
      "email": "dmytro.zhurakovsky@gmail.com"
    }
  ],
  "autoload": {
    "classmap": [
      "Database"
    ],
    "psr-4": {
      "RabbitCMS\\Settings\\": "./"
    },
    "files": [
      "helpers.php"
    ]
  },
  "extra": {
    "module": {
      "name": "settings",
      "namespace": "RabbitCMS\\Settings",
      "enabled": true,
      "providers": "RabbitCMS\\Settings\\Providers\\ModuleProvider"
    }
  }
}
